        return self.llAPI.create_new_airplane(airplane_info_list)

            def create_new_airplane(self, airplane_list):
        self.airplane_list = airplane_list
        return self.airplaneLL.makeAirplane(self.airplane_list)

    def create_new_airplane(self, airplane_str):
        self.airplane_str = airplane_str
        return self.airplaneIO.store_new_airplane_into_file(airplane_str)

with open('daemons.txt', 'w') as fp:
    fp.write('\n'.join('{} {}'.format(x[0],x[1]) for x in mylist)